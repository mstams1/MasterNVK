<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="ToolbarStyle" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Set the background color of the Toolbar -->
        <item name="android:background">#707070</item>

        <!-- Set the text color in the Toolbar -->
        <item name="android:textColorPrimary">#FFFFFF</item>
    </style>

    <style name="AppTheme.PopupOverflow" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Set the background color of the overflow menu -->
        <item name="android:background">#707070</item>
        <item name="android:textColorPrimary">#FFFDFD</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textStyle">italic</item>




    </style>



</resources>